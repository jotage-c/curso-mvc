@model List<Call>

<div class="container">
	<div class="row mb-2">
		<div class="col-10">
			<h3>Clients List</h3>
		</div>
		<div class="col">
			<a class="btn btn-primary" asp-action="Create">Nova Chamada</a>
		</div>
	</div>
	<hr />

	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Id</th>
				<th>Nome</th>
				<th>Email</th>
				<th>Telephone</th>
			</tr>
		</thead>

		<tbody>
			@foreach (var c in Model)
			{
				<tr>
					<td>@c.Id</td>
					<td>@c.CallTitle</td>
					<td>@c.CallStatus</td>
					<td>@c.Client.NameClient</td>
				</tr>
			}
		</tbody>
	</table>
</div>